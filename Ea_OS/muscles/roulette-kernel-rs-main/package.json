{
  "name": "github-copilot-extensions",
  "version": "1.0.0",
  "description": "Roulette Kernel: Verified braid CPU implementation with formal extraction from Lean 4/Coq to Rust. CPU registers as type-safe braid strands, syscalls as T9 sequences, programs as GÃ¶del encodings - mathematically proven correct.",
  "main": "src/main.ts",
  "scripts": {
    "build": "npm run kernel:build",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "kernel:build": "cargo build --release --bin roulette-kernel",
    "kernel:test": "cargo test",
    "kernel:check": "cargo check",
    "kernel:run": "cargo run --release",
    "kernel:lint": "cargo clippy -- -D warnings",
    "kernel:fmt": "cargo fmt --check",
    "kernel:miri": "cargo +nightly miri test",
    "kernel:kani": "cargo kani",
    "kernel:lean": "cd formal-verification && lake build",
    "extract:coq": "echo 'Coq extraction requires MetaCoq - manual translation implemented'",
    "formal:verify": "npm run kernel:kani && npm run kernel:lean",
    "k6:smoke": "k6 run k6-tests/scenarios/smoke-test.js",
    "k6:load": "k6 run k6-tests/scenarios/load-test.js",
    "k6:stress": "k6 run k6-tests/scenarios/stress-test.js",
    "k6:spike": "k6 run k6-tests/scenarios/spike-test.js",
    "k6:api": "k6 run k6-tests/api-test.js",
    "k6:all": "npm run k6:smoke && npm run k6:load && npm run k6:stress",
    "load-test": "npm run k6:load"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.4.0",
    "@octokit/rest": "^20.0.2",
    "crypto": "^1.0.1",
    "eslint": "^8.57.0",
    "googleapis": "^126.0.1",
    "node-cache": "^5.1.2",
    "openai": "^4.20.1",
    "p-retry": "^6.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "jest": "^29.7.0",
    "k6": "^0.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  }
}
