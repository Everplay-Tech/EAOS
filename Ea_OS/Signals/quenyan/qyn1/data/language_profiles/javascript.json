    "boolean": "type:bool",
    "bigint": "type:int",
    "any": "type:any",
    "void": "type:void",
    "never": "type:never",
    "promise": "type:promise",
    "array": "type:array"
  },
  "reverse_type_aliases": {
    "type:string": "string",
    "type:float": "number",
    "type:int": "bigint",
    "type:bool": "boolean",
    "type:any": "any",
    "type:void": "void",
    "type:never": "never",
    "type:promise": "Promise",
    "type:array": "Array"
  },
  "binary_operators": {
    "Add": "op:add",
    "Sub": "op:sub",
    "Mul": "op:mul",
    "Div": "op:div",
    "Mod": "op:mod",
    "BitOr": "op:bit_or",
    "BitAnd": "op:bit_and",
    "BitXor": "op:bit_xor",
    "LShift": "op:shift_left",
    "RShift": "op:shift_right"
  },
  "unary_operators": {
