    "integer": "literal:int",
    "float": "literal:float",
    "template": "literal:template",
    "fallback": "meta:unknown"
  },
  "template_payloads": {
    "template": "literal:template",
    "decorator": "structure:annotation",
    "tagged_template": "literal:template"
  },
  "metadata": {
    "canonicalization": {
      "module": "esm",
      "pragma": "use_strict"
    },
    "morpheme_mappings": {
